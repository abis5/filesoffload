
@*@{
    Layout = null;
}*@


<section class="forms">
    <div class="container">
        <div class="row">
            <div class="col-md-4 ">
                <form name="frm" method="post" enctype="multipart/form-data" action="">
                    <caption>
                        Search (+Wildcard Characters Allowed)
                    </caption>
                    <div class="form-group">
                        <label for="Name">File Content</label>
                        <textarea class="form-control" rows="5" cols="5" placeholder="Address"></textarea>
                    </div>
                    <div class="form-group col-lg-offset-3 ">
                        <input type="submit" name="" value="Search" class="btn btn-success searchbutton" />
                        <input type="submit" value="Reset" name="Reset" class="btn btn-success searchbutton" />
                        <input type="submit" value="Save Search" name="Save Search" class="btn btn-success searchbutton" />
                    </div>
                </form>
            </div>
            <div class="col-md-8">

                <div class="form-group">
                    <label for="Name">File Content</label>
                    <input class="form-control inputcontent" placeholder="Address">
                </div>

                <div class="form-group">
                    <label for="Name">File Keywords</label>
                    <input class="form-control inputcontent" placeholder="Address">
                </div>

                <div class="form-group">
                    <label for="Name">User Id</label>
                    <input class="form-control inputcontent" placeholder="Address">
                </div>
                <div class="form-group">

                    To  &nbsp;&nbsp;<input class="form-control inputcontent" type="date" placeholder="date">
                    From&nbsp;&nbsp; <input class="form-control inputcontent" type="date" placeholder="date">
                </div>





            </div>
        </div>
    </div>
</section>